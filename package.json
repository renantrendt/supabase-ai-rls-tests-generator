{
  "name": "supabase-ai-rls-tests-generator",
  "version": "1.0.12",
  "description": "Claude Sonnet will fetch your RLS policies from Supabase, then generate test cases (including edge cases), run the tests and save the tests and results specific files.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "setup-tests": "./bin/wizard.js",
    "test-rls": "./bin/run-tests.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "rls-test": "ts-node -T rls-test.ts",
    "setup": "node ./bin/wizard.js",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@supabase/supabase-js": "^2.46.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "tsconfig.json",
    "tsconfig.test.json",
    "jest.config.js"
  ],
  "keywords": [
    "supabase",
    "rls",
    "testing",
    "security",
    "ai",
    "claude",
    "anthropic"
  ],
  "author": "Renan Serrano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renantrendt/supabase-ai-rls-tests-generator.git"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}